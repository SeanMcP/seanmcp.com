---
import Note from "../components/Note.astro";
import PageLayout from "./PageLayout.astro";

const {
  frontmatter: { pubDate, edited, number },
} = Astro.props;

const title = `Note #` + number;
---

<PageLayout title={title}>
  <Note date={pubDate} edited={edited} number={number}>
    <slot />
  </Note>
</PageLayout>
